{
  "name": "aeams",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "build:github": "GITHUB_PAGES=true prisma generate && GITHUB_PAGES=true next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset",
    "export": "GITHUB_PAGES=true next build && next export",
    "deploy:github": "npm run build:github",
    "health": "node -e \"console.log('Node:', process.version); console.log('Env vars loaded:', {NEXTAUTH_SECRET: !!process.env.NEXTAUTH_SECRET, DATABASE_URL: !!process.env.DATABASE_URL})\"",
    "env:check": "node -e \"console.log('Environment Check:'); console.log('NEXTAUTH_SECRET:', !!process.env.NEXTAUTH_SECRET); console.log('DATABASE_URL:', !!process.env.DATABASE_URL); console.log('NEXTAUTH_URL:', process.env.NEXTAUTH_URL || 'NOT SET')\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.14.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.5",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.1",
    "next": "15.4.6",
    "next-auth": "^5.0.0-beta.29",
    "openai": "^5.23.1",
    "pg": "^8.16.3",
    "plotly.js-dist-min": "^3.1.0",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.5.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4"
  }
}
